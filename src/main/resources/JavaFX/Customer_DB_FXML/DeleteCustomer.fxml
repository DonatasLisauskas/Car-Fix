      <Label layoutX="28.0" layoutY="111.0" text="Customer ID:">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <TextField fx:id="number" layoutX="134.0" layoutY="111.0" prefHeight="26.0" prefWidth="120.0" />
      <Button layoutX="309.0" layoutY="109.0" onAction="#deleteButton" text="Delete">
